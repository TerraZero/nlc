{
  "services": {
    "nlc.core": {
      "synthetic": true
    },
    "nlc.require": {
      "synthetic": true
    },
    "nlc.logger": {
      "synthetic": true
    }
  },
  "logger": {
    "level": "all",
    "appender": [
      {
        "type": "rollingfile",
        "errorEventName": "error",
        "logDirectory": "./logs",
        "fileNamePattern": "<DATE>.log",
        "dateFormat": "YYYY.MM.DD",
        "level": "all",
        "prettyPrint": true
      }
    ]
  },
  "extensions": [
    "nlc-util",
    "nlc-cli"
  ]
}
